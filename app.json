{
  "expo": {
    "name": "walwalcards",
    "slug": "walwalcards",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "walwalcards",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.delimajohnmark.walwalcards",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FB923C",
        "foregroundImage": "./assets/images/icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.delimajohnmark.walwalcards"
    },
    "web": {
      "output": "static",
      "favicon": "/favicon-32x32.png",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://walwalcards.xyz"
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#FB923C",
          "dark": {
            "backgroundColor": "#FB923C"
          }
        }
      ]
    ],
    "assetBundlePatterns": ["assets/images/*", "assets/fonts/*"],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {
        "origin": "https://walwalcards.xyz"
      },
      "eas": {
        "projectId": "116eb078-c01d-47ce-b30e-b05ba4d5bba6"
      }
    },
    "owner": "delimajohnmark"
  }
}
